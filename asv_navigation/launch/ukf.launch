<?xml version="1.0"?>
<launch>
  <node pkg="robot_localization" type="ukf_localization_node" name="ukf_localization_global" clear_params="true">
    <remap from="diagnostics" to="diagnostics/ukf"/>
    <rosparam command="load" file="$(find asv_navigation)/params/ukf.yaml" />
  </node>
  <!-- TFS -->
  <include file="$(find asv_navigation)/launch/tfs.launch"/>
</launch>
