<?xml version="1.0"?>
<launch>
    <arg name="model" default="$(find asv_launch)/urdf/asv.xacro"/>
	<param name="robot_description" command="$(find xacro)/xacro.py $(arg model)" />
    <!-- Start rviz with a pre-configured AerialMap instance. It will use the fake GPS fix from above. -->
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find asv_launch)/rviz/mission.rviz"/>
</launch>
